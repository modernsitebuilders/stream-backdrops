{
  "functions": {
    "pages/api/sitemap.js": {
      "maxDuration": 10
    }
  },
  "redirects": [
    {
      "source": "/premium",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/category/home-offices",
      "destination": "/category/well-lit",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}